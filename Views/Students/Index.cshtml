@model IEnumerable<test4.Models.Student>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ChineseTeacherAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MathTeacherAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EnglishTeacherAccount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Password)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ChineseTeacherAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MathTeacherAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EnglishTeacherAccount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Account">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Account">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Account">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
